BubbleSort	main.cpp	/^class BubbleSort : public SortStrategy {$/;"	c	file:
CXX	Makefile	/^CXX = c++$/;"	m
CXXFLAGS	Makefile	/^CXXFLAGS = -Wall -Wextra -Werror -std=c++11$/;"	m
NAME	Makefile	/^NAME = a.out$/;"	m
OBJS	Makefile	/^OBJS = $(SRCS:.cpp=.o)$/;"	m
QuickSort	main.cpp	/^class QuickSort : public SortStrategy {$/;"	c	file:
SRCS	Makefile	/^SRCS = main.cpp$/;"	m
SortStrategy	main.cpp	/^class SortStrategy {$/;"	c	file:
Sorter	main.cpp	/^		Sorter(SortStrategy* strategy) : strategy(strategy) {}$/;"	f	class:Sorter
Sorter	main.cpp	/^class Sorter {$/;"	c	file:
main	main.cpp	/^int main(void)$/;"	f
performSort	main.cpp	/^		void performSort(int arr[], int size) {$/;"	f	class:Sorter
quickSort	main.cpp	/^		void quickSort(int arr[], int left, int right) {$/;"	f	class:QuickSort
setStrategy	main.cpp	/^		void setStrategy(SortStrategy* strate) {$/;"	f	class:Sorter
sort	main.cpp	/^		void sort(int arr[], int size) {$/;"	f	class:BubbleSort
sort	main.cpp	/^		void sort(int arr[], int size) {$/;"	f	class:QuickSort
strategy	main.cpp	/^		SortStrategy* strategy;$/;"	m	class:Sorter	file:
~SortStrategy	main.cpp	/^		virtual ~SortStrategy() {}$/;"	f	class:SortStrategy
