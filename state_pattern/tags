!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CXX	Makefile	/^CXX = c++$/;"	m
CXXFLAGS	Makefile	/^CXXFLAGS = -Wall -Wextra -Werror -std=c++11$/;"	m
ConcreteStateA	main.cpp	/^class ConcreteStateA : public State$/;"	c	file:
ConcreteStateB	main.cpp	/^class ConcreteStateB : public State$/;"	c	file:
Context	main.cpp	/^		Context(State* state) : state_(nullptr)$/;"	f	class:Context
Context	main.cpp	/^class Context$/;"	c	file:
NAME	Makefile	/^NAME = a.out$/;"	m
OBJS	Makefile	/^OBJS = $(SRCS:.cpp=.o)$/;"	m
SRCS	Makefile	/^SRCS = main.cpp$/;"	m
State	main.cpp	/^class State$/;"	c	file:
clientCode	main.cpp	/^void clientCode()$/;"	f
context	main.cpp	/^		Context* context;$/;"	m	class:State	file:
handle1	main.cpp	/^void ConcreteStateA::handle1()$/;"	f	class:ConcreteStateA
main	main.cpp	/^int main(void)$/;"	f
override	main.cpp	/^		void handle1() override;$/;"	m	class:ConcreteStateA	file:
request1	main.cpp	/^		void request1()$/;"	f	class:Context
request2	main.cpp	/^		void request2()$/;"	f	class:Context
setContext	main.cpp	/^		void setContext(Context* context)$/;"	f	class:State
state_	main.cpp	/^		State* state_;$/;"	m	class:Context	file:
transitionTo	main.cpp	/^		void transitionTo(State* state)$/;"	f	class:Context
~Context	main.cpp	/^		~Context()$/;"	f	class:Context
~State	main.cpp	/^		virtual ~State() {}$/;"	f	class:State
